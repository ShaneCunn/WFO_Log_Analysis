<Window x:Name="duplicatesWindow" x:Class="WFO_PROJECT.DuplicateOptionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DuplicateOptionsWindow" Height="300" Width="300" Closing="duplicatesWindow_Closing">
    <Grid>
        <ListView x:Name="duplicatesListView" HorizontalAlignment="Left" Height="193" Margin="10,67,0,0" VerticalAlignment="Top" Width="272" Loaded="duplicatesListView_Loaded" SelectionChanged="duplicatesListView_SelectionChanged">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <TextBox HorizontalAlignment="Left" Height="52" Margin="10,10,0,0" TextWrapping="Wrap" Text="There are duplicates in the scripts you are copying, please select which of these you would like to add to the new script." VerticalAlignment="Top" Width="272"/>
    </Grid>
</Window>
